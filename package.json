{
  "name": "hapi-angular-quickstart",
  "version": "0.0.1",
  "description": "Hapi hosting of the Angular quickstart app",
  "main": "server.js",
  "scripts": {
    "setup": "npm install",
    "start": "npm run tsc:projects && npm-run-all --parallel tsc:projectsw start:server ",
    "start:server": "nodemon server.js",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "tsc:projects": "tsc -p angular-quickstart",
    "tsc:projectsw": "tsc -w -p angular-quickstart"
  },
  "keywords": [
    "Hapi",
    "Angular"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.4.0",
    "@angular/compiler": "~2.4.0",
    "@angular/core": "~2.4.0",
    "@angular/forms": "~2.4.0",
    "@angular/http": "~2.4.0",
    "@angular/platform-browser": "~2.4.0",
    "@angular/platform-browser-dynamic": "~2.4.0",
    "@angular/router": "~3.4.0",
    "angular-in-memory-web-api": "~0.2.4",
    "core-js": "^2.4.1",
    "glue": "^4.0.0",
    "good": "^7.1.0",
    "good-console": "^6.2.0",
    "good-squeeze": "^5.0.1",
    "hapi": "^16.1.0",
    "hoek": "^4.1.0",
    "inert": "^4.1.0",
    "rxjs": "5.0.1",
    "systemjs": "0.19.40",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "blipp": "^2.3.0",
    "typescript": "~2.0.10",
    "canonical-path": "0.0.2",
    "http-server": "^0.9.0",
    "lodash": "^4.16.4",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "tslint": "^3.15.1",
    "rimraf": "^2.5.4",
    "@types/node": "^6.0.46"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rjmreis/hapi-api.git"
  }
}
